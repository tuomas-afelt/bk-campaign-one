<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
<meta name="viewport" content="width=device-width,initial-scale=1">

<title>{{ page.title }}</title>
<meta name="keywords" content="{{ page.keywords }}" />
<meta name="description" content="{{ page.description }}" />

<meta property="og:type" content="website" />
{% if profile.logo %}
	<meta property="og:image" content="{{ profile.logo }}" />
	<meta property="og:image:width" content="{{ profile.logowidth }}" />
	<meta property="og:image:height" content="{{ profile.logoheight }}" />
{% endif %}

{% include basekit.headScript %}

<link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">

{% if site.favicon %}
	<link rel="icon" href="{{ site.favicon['favicon'] }}" type="image/x-icon" />
	<link rel="SHORTCUT ICON" href="{{ site.favicon['thumbnail'] }}?v={{ "now"|date("U") }}" type="image/x-icon" />
{% endif %}

